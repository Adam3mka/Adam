if Value then
_G.READY = true
while _G.READY do
wait()
for i,v in pairs(game.Players.LocalPlayer:GetDescendants()) do
if v.Name == "PowerReady" and v:IsA("BoolValue") then
v.Value = true
end
end
end
else
_G.READY = false
end
end})

