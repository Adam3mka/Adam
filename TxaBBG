local Misc = Window:MakeTab({"Misc", "layout-grid"})

Misc:AddSection({"Resolution, Fov, Clumsy"})

Misc:AddTextBox({
Name = "Set you Resolution",
Description = "Best is 0.75",
Default = "",
Callback = function(Value)
    getgenv().Resolution = {
        [".gg/scripters"] = Value
    }
    local Camera = workspace.CurrentCamera
    if getgenv().gg_scripters == nil then
        game:GetService("RunService").RenderStepped:Connect(
            function()
                Camera.CFrame = Camera.CFrame * CFrame.new(0, 0, 0, 1, 0, 0, 0, getgenv().Resolution[".gg/scripters"], 0, 0, 0, 1)
            end)
    end
    getgenv().gg_scripters = "Aori0001"  
end})

Misc:AddTextBox({
Name = "Change Fov",
Description = "Change you Fov",
Default = "",
Callback = function(Value)
    game.Workspace.CurrentCamera.FieldOfView = Value
end})

Misc:AddSection({"Level and XP Changer"})

Misc:AddTextBox({
Name = "Level Changer",
Description = "Tipe Level!",
Default = "",
Callback = function(dm)
    local Targets
    Targets = tonumber(dm)
    wait(0.1)
    local mt = getrawmetatable(game);
setreadonly(mt, false);
local old_index = mt.__index;
mt.__index = function(a, b)
    if tostring(a) == "PPLevel" or tostring(a) == "Level" then
        if tostring(b) == "Value" then
            return Targets;
        end
    end
    return old_index(a, b);
end
end})

Misc:AddTextBox({
Name = "XP Changer",
Description = "Tipe XP!",
Default = "",
Callback = function(dm)
    local Targetz
	Targetz = tonumber(ardmg)
	wait(0.1)
	local mt = getrawmetatable(game);
setreadonly(mt, false);
local old_index = mt.__index;
mt.__index = function(a, b)
    if tostring(a) == "XP" then
        if tostring(b) == "Value" then
            return Targetz;
        end
    end
    return old_index(a, b);
end
end})

