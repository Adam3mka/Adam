    local bold_index = bgmt.__index
    bgmt.__index = function(a, b)
        if tostring(a) == "BCount" then
            if tostring(b) == "Value" then
                return 0
            end
        end
        return bold_index(a, b)
    end
    setreadonly(bgmt, true)

