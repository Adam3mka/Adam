        local A_3 = 0
        local A_4 = Vector3.new(4000000, 0, 4000000)
        local Event = game:GetService("Workspace").FE.Actions.KickG1
        Event:FireServer(A_1, A_2, A_3, A_4)
        end
        end
        end
        end
            
            else
                _G.PStopBall = false
        end
